<Window x:Class="WpfApp1.View.Patientt.Survey.HospitalSurveyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1.View.Patientt.Survey"
        mc:Ignorable="d"
        Title="Hospital Survey Window" Height="450" Width="800">
    <Grid>
        <DataGrid IsReadOnly="True" x:Name ="DataGridAppointments" ItemsSource="{Binding Path=Answers}" AutoGenerateColumns="True" Margin="56,123,56,56"  ColumnWidth="*" RenderTransformOrigin="0.65,0.495"/>
    </Grid>
</Window>
